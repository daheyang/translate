    </item>
    <item android:top="8dp" android:bottom="20dp" android:start="13dp" android:end="13dp">
        <shape android:shape="oval">
            <solid android:color="#CFE7EA"/>
        </shape>
    </item>
    <item android:top="18dp" android:bottom="9dp" android:start="9dp" android:end="9dp">
        <shape android:shape="rectangle">
            <solid android:color="#CFE7EA"/>
            <corners android:topLeftRadius="8dp" android:topRightRadius="8dp"/>
        </shape>
    </item>
</layer-list>